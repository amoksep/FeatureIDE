<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.ovgu.featureide.core.roslaunch.editors.LaunchEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="launch"
            icon="icons/sample.gif"
            id="de.ovgu.featureide.core.roslaunch.editors.LaunchEditor"
            name="Launch Editor">
      </editor>
   </extension>
   <extension
         point="de.ovgu.featureide.core.wizard">
      <newProjectWizard
            class="de.ovgu.featureide.core.wizardextension.DefaultNewFeatureProjectWizardExtension"
            composerid="de.ovgu.featureide.roslaunch"
            id="de.ovgu.featureide.core.roslaunch.newProjectWizard"
            name="de.ovgu.featureide.roslaunch">
      </newProjectWizard>
   </extension>
   <extension
         point="de.ovgu.featureide.core.composers">
      <composer
            class="de.ovgu.featureide.core.roslaunch.FeatureRosComposer"
            description="de.ovgu.featureide.core.roslaunch.composer1"
            id="de.ovgu.featureide.roslaunch.composer"
            name="RosLaunchComposer"
            nature="de.ovgu.featureide.core.roslaunch.composer1">
      </composer>
   </extension>
