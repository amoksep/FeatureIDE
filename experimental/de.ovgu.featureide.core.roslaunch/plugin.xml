   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="de.ovgu.featureide.core.roslaunch.launchConfigurationType"
            icon="icons/sample.gif"
            id="de.ovgu.featureide.core.roslaunch.launchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
