      </configurationPageExtension>
   </extension>
-->
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="de.ovgu.featureide.core.roslaunch.launch.RosLaunchConfigDelegator"
            delegateName="de.ovgu.featureide.core.roslaunch.RosLaunchConfigDelegator"
            id="de.ovgu.featureide.core.roslaunch.launchConfigurationType"
            modes="run,debug"
            name="RosLaunch"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="de.ovgu.featureide.core.roslaunch.launch.LaunchTabGroug"
            id="de.ovgu.featureide.core.roslaunch.launchConfigurationTabGroup"
            type="de.ovgu.featureide.core.roslaunch.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
